<script>

    function displayDiv(reportType, hName) {

        $("#reportType_p").html(reportType);
        $("#report_head").html(hName);

        $("#reportType_np").html(reportType);
        $("#report_nphead").html(hName);

        $("#reportType_cd").html(reportType);
        $("#report_cdhead").html(hName);

        $("#reportType_Inv").html(reportType);
        $("#report_Invhead").html(hName);

        $("#reportType_Com").html(reportType);
        $("#report_Comhead").html(hName);

        if (reportType === 'financial' || reportType === 'corporategovernance' || reportType === 'investorcomplaints' || reportType === 'shareholderpattern' || reportType === 'unclaimeddividend' || reportType === 'policies') {
            $("#DivShowPdf").css('display', 'none');
            $('#searchDiv').css('display', 'none');
            $("#Advertisements").css('display', 'block');
            $("#CodeOfConduct").css('display', 'none');
            $("#Composition").css('display', 'none');
            $("#AnnualRpt").css('display', 'none');
            $("#InvContacts").css('display', 'none');
        }
        else if (reportType === 'meeting' || reportType === 'newspaperadvertisment' || reportType === 'materialevents' || reportType === 'investorrelations' || reportType === 'stockexchange' || reportType === 'subcompany') {
            $('#searchDiv').css('display', 'none');
            $("#CodeOfConduct").css('display', 'none');
            $("#InvContacts").css('display', 'none');
            $("#Composition").css('display', 'none');
            $("#AnnualRpt").css('display', 'none');
            $("#Advertisements").css('display', 'block');
        }
        else if (reportType === 'annualtranswarranty' || reportType === 'shareholdingpattern' || reportType === 'boardmeeting' || reportType === 'general' || reportType === 'subcompany' || reportType === 'annualfinancial' || reportType === 'quarterly' || reportType === 'limitedreviewrpt') {
            $("#DivShowPdf").css('display', 'none');
            $('#searchDiv').css('display', 'none');
            $("#Advertisements").css('display', 'block');
            $("#CodeOfConduct").css('display', 'none');
            $("#InvContacts").css('display', 'none');
            $("#Composition").css('display', 'none');
            $("#AnnualRpt").css('display', 'none');
        }
        else if (reportType === 'investorcontact') {
            $("#DivShowPdf").css('display', 'none');
            $('#searchDiv').css('display', 'none');
            $("#Advertisements").css('display', 'none');
            $("#CodeOfConduct").css('display', 'none');
            $("#Composition").css('display', 'none');
            $("#AnnualRpt").css('display', 'none');
            $("#InvContacts").css('display', 'block');
        }
        else if (reportType === 'composition') {
            $("#DivShowPdf").css('display', 'none');
            $('#searchDiv').css('display', 'none');
            $("#Advertisements").css('display', 'none');
            $("#CodeOfConduct").css('display', 'none');
            $("#InvContacts").css('display', 'none');
            $("#AnnualRpt").css('display', 'none');
            $("#Composition").css('display', 'block');
        }
    }
    function FinFunction() {
        document.getElementById("FinDropdown").classList.toggle("show");
    }
    function AnnualFunction() {
        document.getElementById("AnDropdown").classList.toggle("show");
    }
    function myFunction() {
        document.getElementById("myDropdown").classList.toggle("show");
    }


    // Close the dropdown if the user clicks outside of it
    window.onclick = function (event) {
        if (!event.target.matches('.dropbtn')) {
            var dropdowns = document.getElementsByClassName("dropdown-content");
            var i;
            for (i = 0; i < dropdowns.length; i++) {
                var openDropdown = dropdowns[i];
                if (openDropdown.classList.contains('show')) {
                    openDropdown.classList.remove('show');
                }
            }
        }
    }
</script>
