    function DownloadFile(index) {
        var result = $("#AdvertisementsTbl").find('tr:eq(' + index + ')').find('td:eq(1)').text();
            var ReportType = result.split('_')[0];
        var Sub_cat = result.split('_')[1];

        window.location = '/Transwarranty/DownloadPdf?reportId=' + result + "&type=" + ReportType + "&subOption=" + Sub_cat;
    }
    function Search() {

            var ReportType = $("#reportType_p").html();
            var Pattern = "NA"
            if (ReportType === 'financial') {
                Pattern = ReportType;
                ReportType = 'financial';
            }
            else if (ReportType === 'corporategovernance') {
                Pattern = ReportType;
                ReportType = 'shareholder';
            }
            else if (ReportType === 'investorcomplaints') {
                Pattern = ReportType;
                ReportType = 'shareholder';
            }
            else if (ReportType === 'shareholderpattern') {
                Pattern = ReportType;
                ReportType = 'shareholder';
            }
            var Year = 'NA';
            var Period = 'NA';
            if ((ReportType === 'financial') || (ReportType === 'shareholder' && Pattern === 'corporategovernance') || (ReportType === 'shareholder' && Pattern === 'investorcomplaints')) {
                Year = $("#DdlYear").val();
                Period = $("#DdlQrtr").val();
            }

            AjaxCalls(ReportType, Pattern, Year, Period);
    }
